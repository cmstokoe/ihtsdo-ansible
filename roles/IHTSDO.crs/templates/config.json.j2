{
    "endpoint": {
        "crs": "{{ crs_web_conf_crs_url }}",
        "snowowl": "{{ crs_web_conf_snowowl_url }}",
        "ims": "{{ crs_web_conf_ims_url }}",
        "jira": "{{ crs_web_conf_jira_url }}",
        "ims-api": "{{ crs_web_conf_ims_api_url }}"
    },

    "xhrTimeout": 50000,
    "fileStatusPollingInterval": 5000,
    "messagePollingInterval": 10000,

    "snomed-ct": {
        "path": "snomed-ct/v2/",
        "edition": "MAIN",
        "release": ""
    },

    "sca": {
        "path": "ihtsdo-sca/"
    },

    "jira": {
        "rest-api": {
            "path": "rest/api/",
            "version": "2"
        },
        "author-group": "ihtsdo-sca-author"
    },
    "userRole": {
        "ADMINISTRATOR": "ROLE_ihtsdo-crs-administrators",
        "MANAGER": "ROLE_ihtsdo-crs-managers",
        "STAFF": "ROLE_ihtsdo-crs-staff",
        "MEMBER": "ROLE_ihtsdo-crs-members",
        "PARTNER": "ROLE_ihtsdo-crs-partners",
        "REQUESTOR": "ROLE_ihtsdo-crs-requestors",
        "VIEWER": "ROLE_ihtsdo-crs-viewers"
    }
}