[program:{{relman_app_name}}]
user = {{relman_user_name}}
directory = /opt/%(program_name)s
command = /usr/bin/java -Xms{{relman_xms}} -Xmx{{relman_xmx}}  -jar /opt/%(program_name)s/lib/app.jar --spring.config.location=/opt/%(program_name)s/application.{{relman_conf_type}}
autostart = true
autorestart = true
stdout_logfile_backups=5
stderr_logfile_backups=5
stdout_logfile_maxbytes=10485760
stderr_logfile_maxbytes=10485760
