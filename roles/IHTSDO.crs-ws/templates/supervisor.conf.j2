[program:crs-web]
user = root
directory = /opt/%(program_name)s/run
command = /usr/bin/java -Xms256m -Xmx1024m -XX:PermSize=64m -XX:MaxPermSize=256m -jar /opt/%(program_name)s/lib/app.jar -Dlog.dir=/opt/crs-web/log -DcrsPropertiesFolderLocation=/opt/crs-web/crs-resources -resetExtract -extractDirectory /opt/%(program_name)s/run/extract
autostart = true
autorestart = true
stdout_logfile_backups=5
stderr_logfile_backups=5
stdout_logfile_maxbytes=10485760
stderr_logfile_maxbytes=10485760