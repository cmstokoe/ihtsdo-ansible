[program:snomed-release-service-api]
user = snomed-release-service-api
directory = /var/opt/%(program_name)s/run
command = /usr/bin/java -Xms{{srs_api_xms}} -Xmx{{srs_api_xmx}} -DENV_NAME={{environment_shortname}} -DdataServicePropertiesPath=file:///etc/opt/%(program_name)s/config.properties -jar /opt/%(program_name)s/lib/webapp-runner.jar /opt/%(program_name)s/lib/app.war --port 10000 --path /api --proxy-base-url https://{{ inventory_hostname }}
environment=LANG="en_US.UTF-8",LC_ALL="en_US.UTF-8"
autostart = true
autorestart = true
stdout_logfile_backups=5
stderr_logfile_backups=5
stdout_logfile_maxbytes=10485760
stderr_logfile_maxbytes=10485760
