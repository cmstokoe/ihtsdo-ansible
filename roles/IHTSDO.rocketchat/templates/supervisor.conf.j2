[program:rocketchat]
user = {{ rocketchat_user }}
environment=PORT=3000,MONGO_URL=mongodb://localhost:27017/rocketchat,ROOT_URL={{ rocketchat_url }}
directory = {{ rocketchat_directory }}
command = node main.js
autostart = true
autorestart = true
stdout_logfile_backups=5
stderr_logfile_backups=5
stdout_logfile_maxbytes=10485760
stderr_logfile_maxbytes=10485760
