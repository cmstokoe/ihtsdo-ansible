---
systemchoice: files/systemchoice.yml


nginx_ssl_key_filename: YOUR_KEY.key
nginx_ssl_cert_filename: YOUR_CERT.chained.crt

nginx_ssl_key_file: "files/encrypted/ssl/{{nginx_ssl_key_filename}}"
nginx_ssl_cert_file: "files/encrypted/ssl/{{nginx_ssl_cert_filename}}"
nginx_ssl_cert_file_dest: "/etc/ssl/certs/{{nginx_ssl_cert_filename}}"
nginx_ssl_key_file_dest: "/etc/ssl/private/{{nginx_ssl_key_filename}}"